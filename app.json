{
  "expo": {
    "name": "Drop Cars Admin",
    "slug": "drop-cars-admin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8f14b166-7ecb-41e6-b60e-3a61c21c269f"
      }
    },
    "owner": "dropcars",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/8f14b166-7ecb-41e6-b60e-3a61c21c269f"
    },
    "android": {
      "package": "com.dropcars.dropcarsadmin"
    }
  }
}
